# Copy this file to .env.local and fill in the actual values
# DO NOT commit .env.local to version control

# Database Configuration
DATABASE_URL=postgresql://banking:banking123@database:5432/banking_customer

# PostgreSQL Database Settings (for docker-compose)
POSTGRES_USER=banking
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=banking_customer

# CDP Configuration  
NEXT_PUBLIC_CDP_WRITEKEY=your_cdp_writekey_here
NEXT_PUBLIC_CDP_ENDPOINT=https://your-cdp-endpoint.com

# HCL Interact Configuration
NEXT_PUBLIC_INTERACT_ENDPOINT=your_interact_endpoint_here

# Google Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID=your_ga_measurement_id_here

# Discover Script Configuration
NEXT_PUBLIC_DISCOVER_DEFAULT_SCRIPT=/js/discover.js